package files;
import java.io.FileWriter;
import java.io.PrintWriter;
import java.io.IOException;


public class writer {
	
	String file_path;

	public writer(String file_path) {
		super();
		this.file_path = file_path;
	}

	public String getFile_path() {
		return file_path;
	}

	public void setFile_path(String file_path) {
		this.file_path = file_path;
	}
	
}
