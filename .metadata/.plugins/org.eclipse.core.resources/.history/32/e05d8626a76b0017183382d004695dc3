
public abstract class Vehicle {
	int no_wheels;
	int year;
	int milage;
	
	public Vehicle(int no_wheels, int year, int milage) {
		this.no_wheels = no_wheels;
		this.year = year;
		this.milage = milage;
	}

	
	
	
	public double tire_price() {
		return no_wheels * 25;
	}

	public double milage_cat() {
		double milage_price = 0.0;
		if (milage < 5000)
			milage_price = 50.0;
		else
			milage_price = 100.0;
		return milage_price;
	}
	
	public double Year_price() {
		return year % 1950;
	}
}
