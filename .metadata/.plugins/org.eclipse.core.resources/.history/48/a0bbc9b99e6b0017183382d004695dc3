
public class car extends Vehicle {

	public car(int no_wheels, int year, int milage, int _no_seats, int no_doors, boolean MOT) {
	super(no_wheels, year, milage);
	}

	public double MOT_Price(boolean MOT)
	{
		if (MOT = true)
			{return 50;}
		else {return 0;}
	}
	
	public double doors_Price (int no_doors)
	{
		return no_doors * 5;
	}
}

